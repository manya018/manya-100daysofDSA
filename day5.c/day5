// A system receives two separate logs of user arrival times from two different servers. Each log is already sorted in ascending order. Your task is to create a single chronological log that preserves the correct order of arrivals.

#include <stdio.h>

int main() {
    int a[50], b[50], merged[100];
    int n1, n2, i, j, k;

    printf("Enter number of elements in first log: ");
    scanf("%d", &n1);

    printf("Enter elements (sorted): ");
    for(i = 0; i < n1; i++) {
        scanf("%d", &a[i]);
    }

    printf("Enter number of elements in second log: ");
    scanf("%d", &n2);

    printf("Enter elements (sorted): ");
    for(i = 0; i < n2; i++) {
        scanf("%d", &b[i]);
    }

    i = 0;  
    j = 0;  
    k = 0;  

    while(i < n1 && j < n2) {
        if(a[i] < b[j]) {
            merged[k++] = a[i++];
        } else {
            merged[k++] = b[j++];
        }
    }

    while(i < n1) {
        merged[k++] = a[i++];
    }

    while(j < n2) {
        merged[k++] = b[j++];
    }

    printf("Merged chronological log:\n");
    for(i = 0; i < k; i++) {
        printf("%d ", merged[i]);
    }

    return 0;
}
